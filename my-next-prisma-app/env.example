RAZORPAY_KEY_ID="YOUR_RAZORPAY_KEY_ID"
RAZORPAY_KEY_SECRET="YOUR_RAZORPAY_KEY_SECRET"
RAZORPAY_WEBHOOK_SECRET="YOUR_RAZORPAY_WEBHOOK_SECRET"

# Razorpay Route Platform Account ID
# This is your own account's ID, which you can find in your Razorpay Dashboard.
# It's required to receive the platform commission from split payments.
# Example: acc_XXXXXXXXXXXXXX
RAZORPAY_PLATFORM_ACCOUNT_ID="YOUR_PLATFORM_RAZORPAY_ACCOUNT_ID"

# WebSocket Server
NEXT_PUBLIC_WS_SERVER_URL=http://localhost:4000

# LiveKit Configuration
NEXT_PUBLIC_LIVEKIT_URL=wss://your-livekit-server.com
LIVEKIT_API_KEY=your-livekit-api-key
LIVEKIT_API_SECRET=your-livekit-api-secret

# Redis Configuration (for WebSocket server)
REDIS_URL=redis://localhost:6379
REDIS_PORT=6379

# Room Configuration
ROOM_TTL_MATCH=300
ROOM_TTL_CLAN=2592000
ROOM_TTL_CUSTOM=3600 